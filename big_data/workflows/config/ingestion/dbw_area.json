{
    "extract": {
        "name": "dbw_area_extract",
        "collable": "workflows.spark.common.extract.Extract_API",
        "source": {
            "params":
            {
                "format": "json",
                "language": "{{ env.source_api.gus_dbw.default_language }}",
                "version": "{{ env.source_api.gus_dbw.version }}",
                "category": "area",
                "subcategory": "area"
            },
            "base_url": "{{ env.source_api.gus_dbw.url }}"
        }
    },
    
    "transform": {

    },

    "load": {
        "name": "dbw_area_load",
        "collable": "workflows.spark.common.load.Load_API",
